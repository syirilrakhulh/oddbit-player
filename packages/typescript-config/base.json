{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "removeComments": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "strict": true,
    "target": "esnext"
  }
}
